{
    "exclude": [
        "node_modules",
        "dist",
        "docs",
        "fixtures"
    ],
    "compilerOptions": {
        "lib": [
            "dom",
            "es6"
        ],
        "module": "commonjs",
        "noImplicitAny": false,
        "sourceMap": false,
        "strictNullChecks": false,
        "outDir": "./dist",
        "target": "es5",
        "types": [
            "inflection"
        ]
    },
    "compileOnSave": false
}